{
    "swagger": "2.0",
    "info": {
        "description": "This is an example server",
        "title": "Swagger Auth API",
        "contact": {
            "email": "ndodanli14@gmail.com"
        },
        "version": "1.0"
    },
    "host": "127.0.0.1:5005",
    "paths": {
        "/v1/auth/login": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Login",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "app_user"
                ],
                "summary": "Login",
                "parameters": [
                    {
                        "description": "Username",
                        "name": "loginReq",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/req.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK. On success.",
                        "schema": {
                            "$ref": "#/definitions/baseres.SwaggerSuccessRes-res_LoginRes"
                        }
                    },
                    "400": {
                        "description": "Bad Request. On any validation error.",
                        "schema": {
                            "$ref": "#/definitions/baseres.SwaggerValidationErrRes"
                        }
                    },
                    "401": {
                        "description": "Unauthorized.",
                        "schema": {
                            "$ref": "#/definitions/baseres.SwaggerUnauthorizedErrRes"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error.",
                        "schema": {
                            "$ref": "#/definitions/baseres.SwaggerInternalErrRes"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "baseres.SwaggerInternalErrRes": {
            "type": "object",
            "properties": {
                "m": {
                    "type": "string",
                    "example": "Internal Server Error"
                },
                "s": {
                    "type": "boolean",
                    "example": false
                }
            }
        },
        "baseres.SwaggerSuccessRes-res_LoginRes": {
            "type": "object",
            "properties": {
                "d": {
                    "$ref": "#/definitions/res.LoginRes"
                },
                "m": {
                    "type": "string",
                    "example": "XXX Created/Updated/Deleted Successfully"
                },
                "s": {
                    "type": "boolean",
                    "example": true
                }
            }
        },
        "baseres.SwaggerUnauthorizedErrRes": {
            "type": "object",
            "properties": {
                "m": {
                    "type": "string",
                    "example": "Unauthorized"
                },
                "s": {
                    "type": "boolean",
                    "example": false
                }
            }
        },
        "baseres.SwaggerValidationErrRes": {
            "type": "object",
            "properties": {
                "s": {
                    "type": "boolean",
                    "example": false
                },
                "v": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/baseres.ValidationError"
                    }
                }
            }
        },
        "baseres.ValidationError": {
            "type": "object",
            "properties": {
                "e": {
                    "type": "string",
                    "example": "age must be greater than 0"
                },
                "f": {
                    "type": "string",
                    "example": "age"
                }
            }
        },
        "req.LoginRequest": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "maxLength": 20,
                    "minLength": 6
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "res.LoginRes": {
            "type": "object",
            "properties": {
                "accessToken": {
                    "type": "string"
                },
                "refreshToken": {
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "BearerAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}